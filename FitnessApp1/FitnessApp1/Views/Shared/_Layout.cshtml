    <script>
        $(document).ready(function () {
            $(document).on("keyup", "#search-input", function () {
                console.log("sss")
                let inputVal = $(this).val().trim();
                $("#search-results li").slice(0).remove();
                $.ajax({
                    method: "get",
                    url: "shop/Search?search=" + inputVal,
                    success: function (res) {
                        $("#search-results").append(res);
                    }
                })
            })
        });
    </script>
